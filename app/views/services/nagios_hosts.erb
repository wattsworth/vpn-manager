# Do not modify, this file is generated by VpnManager

<% @groups.each do |group| %>
define hostgroup {
hostgroup_name  <%= group.name %>
alias           <%= group.description %>
members         <%= group.devices.map(&:short_name).join(",") %>
}
<%end %>

<% @devices.each do |device| %>
define host {
use <%=@host_profile%>
host_name <%=device.short_name %>
address <%= device.hostname %>
}
<%end %>